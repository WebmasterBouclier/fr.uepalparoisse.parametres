<?xml version="1.02"?>
<extension key="fr.uepalparoisse.parametres" type="module">
  <file>parametres</file>
  <name>Paramètres de configuration CiviCRM paroisses</name>
  <description>Extension permettant de gérer l'initialisation ou la modification des paramètres CiviCRM pour l'ensemble des paroisses</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Peter JAMES</author>
    <email>webmaster@lebouclier.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/WebmasterBouclier/fr.uepalparoisse.parametres</url>
    <url desc="Documentation">To be provided</url>
    <url desc="Support">To be provided</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2020-10-03</releaseDate>
  <version>1.02</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments></comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Parametres</namespace>
  </civix>
</extension>
